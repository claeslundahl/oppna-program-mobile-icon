<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">

<liferay-portlet-app>
  <portlet>
    <portlet-name>MobileIcon</portlet-name>
    <preferences-unique-per-layout>true</preferences-unique-per-layout>
    <action-url-redirect>true</action-url-redirect>
    <maximize-edit>true</maximize-edit>
    <instanceable>true</instanceable>
    <css-class-wrapper>app-icon</css-class-wrapper>
  </portlet>
  <portlet>
    <portlet-name>MobileSettings</portlet-name>
    <control-panel-entry-category>portal</control-panel-entry-category>
    <control-panel-entry-weight>4.5</control-panel-entry-weight>
  </portlet>
</liferay-portlet-app>